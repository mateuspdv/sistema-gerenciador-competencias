    <changeSet id="20221201225312-1" author="Mateus Padovan Pereira">
        <createTable tableName="seniority">
            <column name="id" type="bigint">
                <constraints primaryKey="true"></constraints>
            </column>
            <column name="name" type="varchar(50)">
